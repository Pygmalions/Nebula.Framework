<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>Nebula Attributing</Title>
        <Authors>Nebula</Authors>
        <Description>A library for scriptable attributes and utilities.</Description>
        <Copyright>MIT License.</Copyright>
        <PackageProjectUrl>https://github.com/NebulaCommittee/Nebula.Framework</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/NebulaCommittee/Nebula.Framework/blob/main/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/NebulaCommittee/Nebula.Framework</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <PackageTags>Nebula; Nebula.Framework; Attribute</PackageTags>
    </PropertyGroup>

</Project>
